   "source": [
    "# Turning Iterative Code to Recursive Code\n",
    "\n",
    "def ite(lst):\n",
    "    for i in lst:\n",
    "        print(i)\n",
    "ite([1, 2, 3])\n",
    "\n",
    "print()\n",
    "\n",
    "def rec(idx, lst):\n",
    "    if idx == len(lst):\n",
    "        return\n",
    "    print(lst[idx])\n",
    "    rec(idx + 1, lst)\n",
    "\n",
    "rec(0, [1, 2, 3])"
   ]
